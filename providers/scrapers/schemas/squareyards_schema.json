{
  "name": "Property Listing",
  "baseSelector": "article.listing-card",
  "fields": [
    {
      "name": "property_id",
      "selector": "article.listing-card",
      "type": "attribute",
      "attribute": "propertyid"
    },
    {
      "name": "title",
      "selector": "h2.heading",
      "type": "text"
    },
    {
      "name": "location",
      "selector": "p.location span",
      "type": "text"
    },
    {
      "name": "price",
      "selector": "p.listing-price strong",
      "type": "text"
    },
    {
      "name": "bedrooms",
      "selector": "li:has(em.icon-bed) span",
      "type": "text"
    },
    {
      "name": "bathrooms",
      "selector": "li:has(em.icon-bed) span",
      "type": "text"
    },
    {
      "name": "area",
      "selector": "li.unit-drop span.unit-value",
      "type": "text"
    },
    {
      "name": "facing",
      "selector": "li:has(em.icon-remedies) span",
      "type": "text"
    },
    {
      "name": "parking",
      "selector": "li:has(em.icon-parking) span",
      "type": "text"
    },
    {
      "name": "flooring",
      "selector": "li:has(em.icon-floor) span",
      "type": "text"
    },
    {
      "name": "furnishing",
      "selector": "li:has(em.icon-semi-furnished) span",
      "type": "text"
    },
    {
      "name": "description",
      "selector": "div.description p",
      "type": "text"
    },
    {
      "name": "image_url",
      "selector": "figure.listing-img div.single-image img.img-responsive",
      "type": "attribute",
      "attribute": "src"
    },
    {
      "name": "agent_name",
      "selector": "div.listing-agent strong.agent-name",
      "type": "text"
    },
    {
      "name": "agent_rating",
      "selector": "div.listing-agent span.rating",
      "type": "text"
    },
    {
      "name": "property_url",
      "selector": "div.listing-body",
      "type": "attribute",
      "attribute": "data-url"
    },
    {
      "name": "price_crore",
      "selector": "div.favorite-btn",
      "type": "attribute",
      "attribute": "data-totalprice"
    },
    {
      "name": "city",
      "selector": "div.favorite-btn",
      "type": "attribute",
      "attribute": "data-city"
    },
    {
      "name": "sublocality",
      "selector": "div.favorite-btn",
      "type": "attribute",
      "attribute": "data-sublocalityname"
    },
    {
      "name": "unit_type",
      "selector": "div.favorite-btn",
      "type": "attribute",
      "attribute": "data-unittype"
    },
    {
      "name": "property_type",
      "selector": "div.favorite-btn",
      "type": "attribute",
      "attribute": "data-propertytype"
    }
  ]
}